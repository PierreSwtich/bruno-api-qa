meta {
  name: Delete newly creted Entry
  type: http
  seq: 5
}

delete {
  url: {{baseURL}}{{booksEndpoint}}/{{newBookId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

assert {
  res.body: isEmpty
}

tests {
    test('Expect status code to be 204', function() {
        expect(res.status).to.equals(204)
    })
    test('Expect response time to be below 1200ms', function() {
        expect(res.responseTime).to.be.below(1200)
    })
}
